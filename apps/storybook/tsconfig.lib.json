{
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ]
  },
  "exclude": [
    "jest.config.ts",
    "stories/**/*.spec.ts",
    "stories/**/*.test.ts",
    "stories/**/*.spec.tsx",
    "stories/**/*.test.tsx",
    "stories/**/*.spec.js",
    "stories/**/*.test.js",
    "stories/**/*.spec.jsx",
    "stories/**/*.test.jsx",
    "**/*.stories.ts",
    "**/*.stories.js",
    "**/*.stories.jsx",
    "**/*.stories.tsx"
  ],
  "extends": "./tsconfig.json",
  "include": [
    "stories/**/*.js",
    "stories/**/*.jsx",
    "stories/**/*.ts",
    "stories/**/*.tsx"
  ]
}
